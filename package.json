{
  "name": "eza",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn codegen && yarn start",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "codegen": "yarn get-schema && yarn generate-types",
    "get-schema": "apollo-codegen introspect-schema http://localhost:3000/graphql --output \"./src/generated/schema.json\"",
    "generate-types": "apollo-codegen generate \"./src/graphql/**/*.ts\" --schema \"./src/generated/schema.json\" --target typescript --output \"./src/generated/types.ts\" --addTypename"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.1",
    "apollo-link-error": "^1.0.7",
    "apollo-link-http": "^1.5.3",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.8.0",
    "react": "^16.3.0",
    "react-apollo": "^2.1.1",
    "react-dom": "^16.3.0",
    "react-scripts-ts": "2.14.0",
    "tslint": "^5.9.1"
  },
  "devDependencies": {
    "@types/graphql": "^0.12.7",
    "@types/jest": "^22.2.2",
    "@types/node": "^9.6.1",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "apollo-codegen": "^0.19.0",
    "typescript": "^2.8.1"
  }
}